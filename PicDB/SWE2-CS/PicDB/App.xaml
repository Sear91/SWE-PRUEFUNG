<Application x:Class="PicDB.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PicDB"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:MainWindowViewModel x:Key="MainWindowViewModel" />
        <local:SearchViewModel x:Key="SearchViewModel"/>
        <local:PhotographerViewModel x:Key="PhotographerViewModel" />
        <local:PhotographerWindowViewModel x:Key="PhotographerWindowViewModel" />
    </Application.Resources>
</Application>
